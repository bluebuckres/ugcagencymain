{
  "name": "makeugc-website",
  "version": "2.0.0",
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.4"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.6",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16"
  },
  "description": "Professional UGC Agency Website - Beautifully Designed & Analytics-Enabled",
  "main": "index.html",
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "./dev-server.sh 8000 & npm run tailwind:watch",
    "dev:alt": "./dev-server.sh 8001 & npm run tailwind:watch",
    "dev:8080": "./dev-server.sh 8080 & npm run tailwind:watch",
    "start": "./dev-server.sh 8080 & npm run tailwind:watch",
    "preview": "./dev-server.sh 3000",
    "tailwind:watch": "tailwindcss -i ./public/assets/css/tailwind-input.css -o ./public/assets/css/tailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./public/assets/css/tailwind-input.css -o ./public/assets/css/tailwind.css --minify",
    "build": "npm run tailwind:build",
    "analytics:setup": "./deployment/scripts/setup-analytics.sh",
    "analytics:start": "cd deployment/docker && docker-compose up -d",
    "analytics:stop": "cd deployment/docker && docker-compose down",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=./public",
    "lint": "echo 'Linting HTML/CSS/JS files...'",
    "format": "echo 'Formatting code...'",
    "test": "echo 'Running tests...'",
    "clean": "echo 'Cleaning build artifacts...'"
  },
  "keywords": [
    "ugc-agency",
    "user-generated-content", 
    "content-marketing",
    "creator-economy",
    "digital-marketing",
    "brand-partnerships",
    "social-media-marketing",
    "content-creation",
    "influencer-marketing"
  ],
  "author": {
    "name": "MakeUGC Team",
    "email": "contact@makeugc.in",
    "url": "https://makeugc.in"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeugc/website.git"
  },
  "homepage": "https://makeugc.in",
  "bugs": {
    "url": "https://github.com/makeugc/website/issues",
    "email": "contact@makeugc.in"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
